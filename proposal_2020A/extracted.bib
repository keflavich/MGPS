	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1801.09801},
	Author = {{Rigby}, A.~J. and {Peretto}, N. and {Adam}, R. and {Ade}, P. and {Andr{\'e}}, P. and {Aussel}, H. and {Beelen}, A. and {Beno{\^\i}t}, A. and {Bracco}, A. and {Bideaud}, A. and {Bourrion}, O. and {Calvo}, M. and {Catalano}, A. and {Clark}, C.~J.~R. and {Comis}, B. and {De Petris}, M. and {D{\'e}sert}, F.-X. and {Doyle}, S. and {Driessen}, E.~F.~C. and {Goupy}, J. and {Kramer}, C. and {Lagache}, G. and {Leclercq}, S. and {Lestrade}, J.-F. and {Mac{\'{\i}}as-P{\'e}rez}, J.~F. and {Mauskopf}, P. and {Mayet}, F. and {Monfardini}, A. and {Pascale}, E. and {Perotto}, L. and {Pisano}, G. and {Ponthieu}, N. and {Rev{\'e}ret}, V. and {Ritacco}, A. and {Romero}, C. and {Roussel}, H. and {Ruppin}, F. and {Schuster}, K. and {Sievers}, A. and {Triqueneaux}, S. and {Tucker}, C. and {Zylka}, R.},
	Date-Added = {2019-07-31 21:32:27 -0400},
